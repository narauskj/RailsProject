<p id="notice"><%= notice %></p>

<h1>Listing Assets</h1>

<table>
  <thead>
    <tr>
      <th>Asset type</th>
      <th>Asset tag</th>
      <th>Serial nr</th>
      <th>Po</th>
      <th>Po date</th>
      <th>Price</th>
      <th>Lifecycle</th>
      <th>Make</th>
      <th>Model</th>
      <th>Is used</th>
      <th>Is assigned</th>
      <th>Is supported</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assets.each do |asset| %>
      <tr>
        <td><%= asset.asset_type %></td>
        <td><%= asset.asset_tag %></td>
        <td><%= asset.serial_nr %></td>
        <td><%= asset.po %></td>
        <td><%= asset.po_date %></td>
        <td><%= asset.price %></td>
        <td><%= asset.lifecycle %></td>
        <td><%= asset.make %></td>
        <td><%= asset.model %></td>
        <td><%= asset.is_used %></td>
        <td><%= asset.is_assigned %></td>
        <td><%= asset.is_supported %></td>
        <td><%= link_to 'Show', asset %></td>
        <td><%= link_to 'Edit', edit_asset_path(asset) %></td>
        <td><%= link_to 'Destroy', asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asset', new_asset_path %>
